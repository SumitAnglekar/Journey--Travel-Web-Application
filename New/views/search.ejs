    </div>
  
  <div class="col-sm-8 sidenav">
            
  <table class="table table-hover">
    <thead>
      <tr>
        <th><h2>Airline Name</h2></th>
        <th><h2>Time</h2></th>
        <th><h2>Price</h2></th>
      </tr>
    </thead>
    <tbody style="font-size:20px;">
      <% flights.forEach(function(flight){ %>

       <tr>
        
        <td><img src="<%= flight.AirlineImage %>" class="img-circle" alt="Cinque Terre" width="100" height="100"><div> <%=flight.AirlineName %></div></td>
        <td><%=flight.DepartureTime %> -  <%=flight.ArrivalTime %></td>
        <td><%=flight.TicketPrice %></td>
        <td><a href="/passenger/<%=flight._id%>"><button type="button" class="btn btn-primary btn-lg" onClick='send()'>Book</button></td></a>
      </tr>

      <% }) %>
    </tbody>
  </table>
</div>
